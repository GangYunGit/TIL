<Window x:Class="FileUploadTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FileUploadTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="500">
    <Grid VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button x:Name="buttonOpenWindow" Content="새 창 열기" HorizontalAlignment="Center" VerticalAlignment="Top" Width="100" Height="40" Click="BtnOpenWindow_Click" Margin="0,7,0,0"/>
        <Button x:Name="buttonOpenFile" Content="파일 업로드" HorizontalAlignment="Center" Margin="0,52,0,0" VerticalAlignment="Top" Width="100" Height="40" Click="BtnOpenFile_Click"/>
        <TextBox x:Name="textBoxViewFile" HorizontalAlignment="Center" Margin="0,97,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="400" Height="400"/>
    </Grid>
</Window>
